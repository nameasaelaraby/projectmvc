@model DAL.ViewModels.BranchVM

@{
    ViewData["Title"] = "Branch Details";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>Branch Details</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BranchName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BranchName)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.BranchLocation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.BranchLocation)
        </dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-id="@Model.BranchId" class="btn btn-warning">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>
